{
	"name": "rest-boilerplate-ts",
	"version": "0.6.0",
	"main": "index.ts",
	"scripts": {
		"prepare": "husky || true",
		"build": "rm -fr dist/* && npx tsc",
		"start": "node dist/index.js",
		"start:docker": "docker build -t knitto_boiler_plate:latest --build-arg github_token=${GITHUB_TOKEN} . && docker compose up -d",
		"stop:docker": "docker compose down",
		"dev": "tsx watch ./src/index.ts",
		"lint": "eslint --fix **/*.ts",
		"test": "jest"
	},
	"keywords": [],
	"author": "Knitto Textile Dept IT",
	"license": "MIT",
	"dependencies": {
		"@knittotextile/knitto-core-backend": "~0.1.2",
		"@knittotextile/knitto-http": "~0.1.5",
		"@knittotextile/knitto-mysql": "~0.1.5",
		"@knittotextile/knitto-rabbitmq": "^1.0.0",
		"bcryptjs": "~2.4.3",
		"dotenv": "~16.4.5",
		"express-jsdoc-swagger": "^1.8.0",
		"jsonwebtoken": "~9.0.0",
		"lodash": "^4.17.21",
		"module-alias": "~2.2.3",
		"moment": "^2.30.1",
		"node-singleflight": "^0.1.2",
		"tslib": "~2.6.2",
		"valibot": "^0.36.0"
	},
	"devDependencies": {
		"@babel/core": "~7.23.7",
		"@babel/plugin-proposal-decorators": "~7.23.7",
		"@babel/preset-env": "~7.23.7",
		"@babel/preset-typescript": "~7.23.3",
		"@types/babel__core": "~7.20.5",
		"@types/babel__preset-env": "~7.9.6",
		"@types/bcryptjs": "~2.4.2",
		"@types/jest": "~29.5.11",
		"@types/jsonwebtoken": "~9.0.2",
		"@types/module-alias": "~2.0.4",
		"@types/node": "~20.10.4",
		"babel-jest": "~29.7.0",
		"eslint": "~8.56.0",
		"eslint-config-love": "~57.0.0",
		"husky": "~9.1.1",
		"jest": "~29.7.0",
		"module-alias-jest": "~0.0.3",
		"tsx": "~4.16.2",
		"typescript": "~5.5.4"
	},
	"_aliases": {
		"@root": "./src/",
		"@http": "./src/app/http/",
		"@httpController": "./src/app/http/controller/",
		"@repositories": "./src/repositories/",
		"@libs": "./src/libs/",
		"@services": "./src/services/"
	}
}
